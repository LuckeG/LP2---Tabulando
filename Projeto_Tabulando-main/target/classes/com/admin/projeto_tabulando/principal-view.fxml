<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" prefHeight="300.0" prefWidth="773.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.admin.projeto_tabulando.controller.PrincipalController">
    <children>
        <Button fx:id="BotaoSair" alignment="CENTER" layoutX="512.0" layoutY="405.0" mnemonicParsing="false" onAction="#menuItemSairOnClicked" opacity="0.83" prefHeight="25.0" prefWidth="100.0" style="-fx-background-radius: 8px; -fx-background-color: f88622;" text="Sair" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS" size="15.0" />
            </font>
        </Button>
        <Button fx:id="buscar" alignment="CENTER" layoutX="462.0" layoutY="182.0" mnemonicParsing="false" onMouseClicked="#menuBuscarJogoOnClicked" opacity="0.83" prefHeight="51.0" prefWidth="200.0" style="-fx-background-color: f88622; -fx-background-radius: 8px;" text="Entrar em um jogo" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS" size="20.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="462.0" layoutY="57.0" opacity="0.96" prefHeight="45.0" prefWidth="200.0" text="BEM VINDO!" textAlignment="CENTER" textFill="#e16302">
            <font>
                <Font name="Gabriola" size="29.0" />
            </font>
        </Label>
        <ImageView fitHeight="510.0" fitWidth="344.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../img/nova.jpeg" />
            </image>
        </ImageView>
      <Label fx:id="nome" alignment="CENTER" contentDisplay="CENTER" layoutX="557.0" layoutY="102.0" style="-fx-background-radius: 5px; -fx-border-width: 5; -fx-border-radius: 5;" textAlignment="CENTER" textFill="#e16302">
         <font>
            <Font name="Gabriola" size="29.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
